    <td>{{this.addr1}}</td>
    <td>{{this.phone_number}}</td>
    <td>
        <a href="#" type="button">View</a>
        <a href="#" type="button">Edit</a>
        <a href="#" type="button">Fire</a>
    </td>
</tr>
{{/each}}
</tbody>
</table>