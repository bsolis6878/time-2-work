<div class="signed-in pl-5 pt-4">
    {{#if loggedIn}}
<h1>Current Signed in as: {{name}}</h1>
{{/if}}
</div>

<div class="company"></div>

<div role="team-lead" class="is-size-5 pl-5 pt-4">Role: Team Leader</div>

<div class="manage-hours"></div>

<div class="manage-employees pl-5 pt-4">
    <table class="user-table table is-bordered is-striped">
    <thred>
       <th>#</th>
       <th>Username</th>
       <th>Email</th>
       <th>Name</th>
       <th>Address</th>
       <th>Phone</th>
    </thred>

<tbody>
{{#each users}}
<tr>
    <th>{{this.id}}</th>
    <td>{{this.username}}</td>
    <td>{{this.email}}</td>
    <td>{{this.name}}</td>
    <td>{{this.addr1}}</td>
    <td>{{this.phone_number}}</td>
</tr>
{{/each}}
</tbody>
</table>
</div>
